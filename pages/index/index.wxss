/**index.wxss**/
.weui-cells{
    background-color: #FFFFFF;  
    font-size: 17px;
    width:100%;
}

.weui-cell {
    display: flex;  
    flex-direction:row;
    align-items: center;
    border-bottom:1px solid #ccc;
    padding:16rpx 20rpx;
}
.weui-cell__hd{  
  width:30px;
  height: 24px;
}

.weui-cell__bd{   
    flex: 1;
    line-height: 24px;
}

.weui-cell__ft {
    text-align: right;
    color: #999999;   
    line-height: 24px; 
  }

.blueCar{
    display: inline-block;
    height: 25px;
    width: 100px;
    text-align: center;
    font: normal lighter 16px/25px arial, sans-serif;   
    background-image: url(data:image/png;base64,);
    background-size: 100px 25px;
}

.whiteCar{
    display: inline-block;
    height: 25px;
    width: 100px;
    text-align: center;
    font: normal lighter 16px/25px arial, sans-serif;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100px 25px;
}

.yellowCar{
    display: inline-block;
    height: 25px;
    width: 100px;
    text-align: center;
    font: normal lighter 16px/25px arial, sans-serif;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100px 25px;
}

.blackCar{
    display: inline-block;
    height: 25px;
    width: 100px;
    text-align: center;
    font: normal lighter 16px/25px arial, sans-serif;
    background-color:#ccc;
    background-image: url(data:image/png;base64,);
    background-size: 100px 25px;
    color: #fff;
}

.addicon{
   width:25px;
   height:25px;
}

.myTripsicon{
   width:30px;
   height:30px;
}

.center_text{
   justify-content: center;
}